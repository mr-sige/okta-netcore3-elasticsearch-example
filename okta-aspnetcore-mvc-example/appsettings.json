{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Okta": {
    "OktaDomain": "https://{yourDevUrl}.okta.com/",
    "ClientId": "{yourClientId}",
    "ClientSecret": "{yourClientSecret}"
  },
  "ElasticCloud": {
    "Endpoint": "https://{yourElasticDeploymentUrl}.gcp.cloud.es.io:9243",
    "BasicAuthUser": "elastic",
    "BasicAuthPassword": "{YourPassword}"
  }
}
